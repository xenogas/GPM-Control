{
    "manifest_version": 2,
    "name": "Embed Test",
    "version": "1.0",

    "content_scripts": [
        {
            "matches": ["*://play.google.com/*"],
            "js": [
                "content.js"
            ]
        }
    ],

    "background": {
        "scripts": [
            "track.js",
            "background.js"
        ]
    },

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],

    "web_accessible_resources": [
        "track.js",
        "injected.js"
    ]
}